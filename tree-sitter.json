{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rasi",
      "camelcase": "Rasi",
      "title": "Rasi",
      "scope": "source.rasi",
      "file-types": [
        "rasi"
      ],
      "injection-regex": "^rasi$",
      "class-name": "TreeSitterRasi"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Treesitter grammar for Rofi theme files",
    "authors": [
      {
        "name": "Pierrick Guillaume",
        "email": "pguillaume@fymyte.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Fymyte/tree-sitter-rasi"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}